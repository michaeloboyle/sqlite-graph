      "enabled": true,
      "auto_label": true,
      "auto_assign": false
    },
    "sparc": {
      "enabled": true,
      "memory_guided": true,
      "optimization_enabled": true
    },
    "claude_skills": {
      "enabled": true,
      "agentdb_context": true
    }
  }
